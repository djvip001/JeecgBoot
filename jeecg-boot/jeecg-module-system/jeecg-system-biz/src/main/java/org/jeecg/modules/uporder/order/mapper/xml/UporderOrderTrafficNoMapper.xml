<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.uporder.order.mapper.UporderOrderTrafficNoMapper">

	<delete id="deleteByMainId" parameterType="java.lang.String">
		DELETE 
		FROM  uporder_order_traffic_no 
		WHERE
			 order_id = #{mainId} 	</delete>
	
	<select id="selectByMainId" parameterType="java.lang.String" resultType="org.jeecg.modules.uporder.order.entity.UporderOrderTrafficNo">
		SELECT * 
		FROM  uporder_order_traffic_no
		WHERE
			 order_id = #{mainId} 	</select>
</mapper>
