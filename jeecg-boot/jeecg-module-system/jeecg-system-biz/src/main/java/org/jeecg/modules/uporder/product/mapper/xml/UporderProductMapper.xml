<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.uporder.product.mapper.UporderProductMapper">

    <update id="recover">
        update uporder_product set del_flag=0 where id=#{id}

    </update>

    <select id="delList" resultType="org.jeecg.modules.uporder.product.entity.UporderProduct">
        select * from uporder_product ${ew.customSqlSegment}

    </select>
</mapper>