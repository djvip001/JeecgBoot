<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.uporder.product.mapper.UporderProductUserLimitMapper">

	<delete id="deleteByMainId" parameterType="java.lang.String">
		DELETE 
		FROM  uporder_product_user_limit 
		WHERE
			 product_id = #{mainId} 	</delete>
	
	<select id="selectByMainId" parameterType="java.lang.String" resultType="org.jeecg.modules.uporder.product.entity.UporderProductUserLimit">
		SELECT * 
		FROM  uporder_product_user_limit
		WHERE
			 product_id = #{mainId} 	</select>
</mapper>
