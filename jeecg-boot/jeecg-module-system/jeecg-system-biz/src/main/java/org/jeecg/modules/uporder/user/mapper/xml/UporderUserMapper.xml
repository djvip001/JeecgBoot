<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.uporder.user.mapper.UporderUserMapper">

    <select id="delList" resultType="org.jeecg.modules.uporder.user.entity.UporderUser">
        select * from uporder_user ${ew.customSqlSegment}

    </select>

    <update id="recover" >
        update uporder_user set del_flag=0 where id=#{id}
    </update>
</mapper>